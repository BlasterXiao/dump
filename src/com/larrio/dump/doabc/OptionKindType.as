package com.larrio.dump.doabc
{
	
	/**
	 * 可选传参类型常量
	 * @author larryhou
	 * @createTime Dec 16, 2012 6:47:27 PM
	 */
	public class OptionKindType
	{
		/**
		 * 双精度浮点型 
		 */		
		public static const DOUBLE              :uint = 0x6;  // 6
		
		/**
		 * explicit namespace 
		 */		
		public static const EXPLICIT_NAMESPACE  :uint = 0x19; // 25
		
		/**
		 * 布尔型：false 
		 */		
		public static const FALSE               :uint = 0xA;  // 10
		
		/**
		 * 有符号整形 
		 */		
		public static const INT                 :uint = 0x3;  // 3
		
		/**
		 * namespace 
		 */		
		public static const NAMESPACE           :uint = 0x8;  // 8
		
		/**
		 * null 
		 */		
		public static const NULL                :uint = 0xC;  // 12
		
		/**
		 * package internal namespace 
		 */		
		public static const PACKAGE_INTERNAL_NS :uint = 0x17; // 23
		
		/**
		 * package namespace 
		 */		
		public static const PACKAGE_NAMESPACE   :uint = 0x16; // 22
		
		/**
		 * private 
		 */		
		public static const PRIVATE_NS          :uint = 0x5;  // 5
		
		/**
		 * protected 
		 */		
		public static const PROTECTED_NAMESPACE :uint = 0x18; // 24
		
		/**
		 * static protected 
		 */		
		public static const STATIC_PROTECTED_NS :uint = 0x1A; // 26
		
		/**
		 * 布尔型：true 
		 */		
		public static const TRUE                :uint = 0xB;  // 11
		
		/**
		 * 无符号整形 
		 */		
		public static const UINT                :uint = 0x4;  // 4
		
		/**
		 * undefined 
		 */		
		public static const UNDEFINED           :uint = 0x0;  // 0
		
		/**
		 * utf8字符串 
		 */		
		public static const UTF8                :uint = 0x1;  // 1
	}
}